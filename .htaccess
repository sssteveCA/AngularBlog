<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} ^/api/v1
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /src/assets/php/index.php [L]
</IfModule>

Options -Indexes

<Files .env>
    Order allow,deny
    Deny from all
</Files>